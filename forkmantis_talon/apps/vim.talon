vim save:
    key(esc)
    insert(":w")
    key(enter)

vim save quit:
    key(esc)
    insert(":wq")
    key(enter)

vim force quit:
    key(esc)
    insert(":q!")
    key(enter)

vim open file:
    key(esc)
    insert(" s")

vim change file:
    key(esc)
    insert(" \n")

vim easy find:
    insert("  f")
